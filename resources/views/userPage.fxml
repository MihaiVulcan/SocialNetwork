<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.ui.gui.UserPageController">
   <children>
      <VBox layoutX="200.0" layoutY="83.0">
         <children>
            <Button fx:id="btMessages" mnemonicParsing="false" onMouseClicked="#handleMessages" prefHeight="30.0" prefWidth="200.0" text="Messages">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="btFriends" mnemonicParsing="false" onMouseClicked="#handleFriends" prefHeight="30.0" prefWidth="200.0" text="Friends">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btFriendRequsts" mnemonicParsing="false" onMouseClicked="#handleRequests" prefHeight="30.0" prefWidth="200.0" text="Friend Requests">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btActivity" mnemonicParsing="false" onMouseClicked="#handleReports" prefHeight="30.0" prefWidth="200.0" text="Activity reports">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="btEvents" mnemonicParsing="false" onMouseClicked="#handleEvents" prefHeight="30.0" prefWidth="200.0" text="Events">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btLogout" layoutX="10.0" layoutY="41.0" mnemonicParsing="false" onMouseClicked="#handleLogout" prefHeight="30.0" prefWidth="200.0" text="Log out">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
